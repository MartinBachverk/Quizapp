<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.opilane.simplequiz.HighestScoreActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="132dp"
        android:layout_height="49dp"
        android:layout_marginLeft="-26dp"
        android:layout_marginTop="134dp"
        android:onClick="onClick"
        android:text="Try again"
        app:layout_constraintLeft_toLeftOf="@+id/sinuScore"
        app:layout_constraintTop_toBottomOf="@+id/sinuScore"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/Highscore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="129dp"
        android:layout_marginStart="129dp"
        android:layout_marginTop="172dp"
        android:gravity="center"
        android:text="Best score: "
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/sinuScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Your score:"
        app:layout_constraintLeft_toLeftOf="@+id/Highscore"
        app:layout_constraintTop_toBottomOf="@+id/Highscore"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginLeft="3dp" />

    <Button
        android:id="@+id/vastused"
        android:layout_width="132dp"
        android:layout_height="49dp"
        android:onClick="vastusedClick"
        android:text="Answers "
        tools:layout_editor_absoluteY="274dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.523" />
</android.support.constraint.ConstraintLayout>
